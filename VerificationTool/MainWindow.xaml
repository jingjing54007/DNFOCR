<Window x:Class="VerificationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VerificationTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Loaded="FormLoad">
        <Image x:Name="imageLable" HorizontalAlignment="Left" Height="50" Margin="33,28,0,0" VerticalAlignment="Top" Width="90"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="46,147,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="438"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="115,190,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="235" GotFocus="TextFocused"/>
        <Button x:Name="button" Content="Update" HorizontalAlignment="Left" Margin="115,271,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Skip" HorizontalAlignment="Left" Margin="245,271,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"  />
    </Grid>
</Window>
